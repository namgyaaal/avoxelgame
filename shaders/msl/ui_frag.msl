#include <metal_stdlib>
#include <simd/simd.h>

using namespace metal;

struct main0_out
{
    float4 o_color [[color(0)]];
};

struct main0_in
{
    float3 uv [[user(locn1)]];
};

fragment main0_out main0(main0_in in [[stage_in]], texture2d_array<float> sample_tex [[texture(0)]], sampler sample_texSmplr [[sampler(0)]])
{
    main0_out out = {};
    out.o_color = sample_tex.sample(sample_texSmplr, in.uv.xy, uint(rint(in.uv.z)));
    return out;
}

